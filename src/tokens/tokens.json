{
  "color": {
    "$type": "color",
    "primary": {
      "$value": "#007bff"
    },
    "secondary": {
      "$value": "#6c757d"
    }
  },
  "spacing": {
    "$type": "spacing",
    "none": {
      "$value": "0"
    },
    "small": {
      "$value": "0.5rem"
    },
    "medium": {
      "$value": "1rem"
    }
  },
  "typography": {
    "font": {
      "family": {
        "$type": "fontFamily",
        "body": {
          "$value": "Roboto, sans-serif"
        }
      },
      "size": {
        "$type": "fontSize",
        "base": {
          "$value": "1rem"
        },
        "lg": {
          "$value": "1.25rem"
        }
      }
    }
  }
}